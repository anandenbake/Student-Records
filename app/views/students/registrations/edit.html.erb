<h2>Edit <%= resource_name.to_s.humanize %></h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>

  <%= devise_error_messages! %>

   <div class="feild">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </div><br />

  <div class="feild">
    <%= f.label :lastt_name %><br />
    <%= f.text_field :last_name %>
  </div><br />

  <div class="feild">
    <%= f.label :phonenum %><br />
    <%= f.text_field :phonenum %>
  </div><br />

  <div class="feild">
    <%= f.label :college_name %><br />
    <%= f.text_field :college_name %>
  </div><br />

  <div class="feild">
    <%= f.label :objective %><br />
    <%= f.text_area :objective %>
  </div><br />

  <div class="feild">
    <%= f.label :Education %><br />
    <%= f.text_field :Education %>
  </div><br />

  <div class="feild">
    <%= f.label :percentage %><br />
    <%= f.text_field :percentage %>
  </div><br />

  <div class="feild">
    <%= f.label :passout_year %><br />
    <%= f.text_field :passout_year %>
  </div><br />

  <div class="feild">
    <%= f.label :Technical_skills %><br />
    <%= f.text_area :Technical_skills %>
  </div><br />

  <div class="feild">
    <%= f.label :Project_description %><br />
    <%= f.text_area :Project_description %>
  </div><br />

  <div class="feild">
    <%= f.label :Tranning_undergone %><br />
    <%= f.text_area :Tranning_undergone %>
  </div><br />

  <div class="feild">
    <%= f.label :Hobbies %><br />
    <%= f.text_area :Hobbies %>
  </div><br />

  <div class="feild">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div><br />

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <% if devise_mapping.confirmable? && resource.pending_reconfirmation? %>
    <div>Currently waiting confirmation for: <%= resource.unconfirmed_email %></div>
  <% end %>

  <div class="field">
    <%= f.label :password %> <i>(leave blank if you don't want to change it)</i><br />
    <%= f.password_field :password, autocomplete: "off" %>
    <% if @minimum_password_length %>
      <br />
      <em><%= @minimum_password_length %> characters minimum</em>
    <% end %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %>
  </div>

  <div class="field">
    <%= f.label :current_password %> <i>(we need your current password to confirm your changes)</i><br />
    <%= f.password_field :current_password, autocomplete: "off" %>
  </div>

  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>

<h3>Cancel my account</h3>

<p>Unhappy? <%= button_to "Cancel my account", registration_path(resource_name), data: { confirm: "Are you sure?" }, method: :delete %></p>

<%= link_to "Back", :back %>
